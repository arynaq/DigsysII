float areal(float s1, float s2, float s3){
  float s = (s1+s2+s3)/2;
  float a = sqrt(s*(s-s1)*(s-s2)*(s-s3));
  return a;
}


int factorial(int a){
  if(a==1)
    return 1;
  return a*factorial(a-1);
}

void oppgave1(void){
  printf("\n%10s %10s %10s", "i", "i*i", "i*i*i\n");
  for(int i=1; i<=10; i++){
    printf("\n %10d %10d %10d", i, i*i, i*i*i);
  }
}
